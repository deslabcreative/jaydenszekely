fset":917}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Reducing insulin levels. ","position":{"start":{"line":7,"column":1,"offset":919},"end":{"line":7,"column":28,"offset":946}}}],"position":{"start":{"line":7,"column":1,"offset":919},"end":{"line":7,"column":28,"offset":946}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Upregulating p53 - a protein that protects our DNA from damage.","position":{"start":{"line":9,"column":1,"offset":948},"end":{"line":9,"column":66,"offset":1013}}}],"position":{"start":{"line":9,"column":1,"offset":948},"end":{"line":9,"column":66,"offset":1013}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Strengthening our immune system.","position":{"start":{"line":11,"column":1,"offset":1015},"end":{"line":11,"column":35,"offset":1049}}}],"position":{"start":{"line":11,"column":1,"offset":1015},"end":{"line":11,"column":35,"offset":1049}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Improving our circulation.","position":{"start":{"line":13,"column":1,"offset":1051},"end":{"line":13,"column":29,"offset":1079}}}],"position":{"start":{"line":13,"column":1,"offset":1051},"end":{"line":13,"column":29,"offset":1079}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Reducing inflammation.","position":{"start":{"line":15,"column":1,"offset":1081},"end":{"line":15,"column":25,"offset":1105}}}],"position":{"start":{"line":15,"column":1,"offset":1081},"end":{"line":15,"column":25,"offset":1105}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"∙ Reducing stress and high cortisol levels.","position":{"start":{"line":17,"column":1,"offset":1107